{
	"name": "corvmc/state-management",
	"description": "A powerful, flexible state management system for Laravel models with seamless Filament integration",
	"type": "laravel-module",
	"version": "1.0",
	"license": "proprietary",
	"require": {
		"php": "^8.1",
		"filament/filament": "^3.0"
	},
	"autoload": {
		"psr-4": {
			"CorvMC\\StateManagement\\": "src/",
			"CorvMC\\StateManagement\\Tests\\": "tests/",
			"CorvMC\\StateManagement\\Database\\Factories\\": "database/factories/",
			"CorvMC\\StateManagement\\Database\\Seeders\\": "database/seeders/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"laravel": {
			"providers": [
				"CorvMC\\StateManagement\\StateManagementServiceProvider"
			]
		}
	}
}
