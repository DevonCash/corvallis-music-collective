{
	"name": "corvmc/productions",
	"description": "Productions module for managing music concerts and events",
	"type": "laravel-module",
	"require": {
		"php": "^8.1"
	},
	"autoload": {
		"psr-4": {
			"CorvMC\\Productions\\": "src/",
			"CorvMC\\Productions\\Tests\\": "tests/",
			"CorvMC\\Productions\\Database\\Factories\\": "database/factories/",
			"CorvMC\\Productions\\Database\\Seeders\\": "database/seeders/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"CorvMC\\Productions\\ProductionsServiceProvider"
			]
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
