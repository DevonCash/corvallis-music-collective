{
	"name": "corvmc/finance",
	"description": "Finance module for CorvMC",
	"type": "library",
	"version": "1.0",
	"license": "MIT",
	"require": {
		"php": "^8.2"
	},
	"autoload": {
		"psr-4": {
			"CorvMC\\Finance\\": "src/",
			"CorvMC\\Finance\\Tests\\": "tests/",
			"CorvMC\\Finance\\Database\\Factories\\": "database/factories/",
			"CorvMC\\Finance\\Database\\Seeders\\": "database/seeders/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CorvMC\\Finance\\Tests\\": "tests/"
		}
	},
	"minimum-stability": "stable",
	"extra": {
		"laravel": {
			"providers": [
				"CorvMC\\Finance\\Providers\\FinanceServiceProvider"
			]
		}
	}
}
